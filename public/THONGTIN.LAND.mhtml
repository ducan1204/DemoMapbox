            $.ajax({
                url:"https://thongtin.land/change-tinh/"+tinh,
                success:function (data) {
                    $('#'+$(e).attr('data-qh')).empty();
                    var text = '';
                    $.each(data,function(index,value){
                        text+='<option value="'+index+'">'+value+'</option>'
                    })
                    text+='<option value="" selected>- Quận / Huyện - </option>'
                    $('#'+$(e).attr('data-qh')).append(text);
                    $('#'+$(e).attr('data-qh')).select2();
